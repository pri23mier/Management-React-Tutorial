{
    "name": "management",
    "version": "1.0.0",
    "scripts": {
        "client": "cd client && npm start",
        "server": "nodemon server.js",
        "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
        "start": "webpack-dev-server --open --config webpack/webpack.config.dev.js"
    },
    "dependencies": {
        "@material-ui/icons": "^4.9.1",
        "body-parser": "^1.20.1",
        "express": "^4.18.2",
        "multer": "^1.4.2"
    },
    "devDependencies": {
        "concurrently": "^4.0.1",
        "nodemon": "^2.0.2"
    }
}
